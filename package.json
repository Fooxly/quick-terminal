{
	"name": "quick-terminal",
	"displayName": "Quick Terminal",
	"author": "Fooxly <hello@fooxly.com> (https://developers.fooxly.com/)",
	"description": "Open an external terminal by the press of a button",
	"version": "0.1.3",
	"keywords": [
		"fooxly",
		"vscode",
		"code",
		"button",
		"statusbar",
		"terminal",
		"native"
	],
	"homepage": "https://github.com/Fooxly/quick-terminal",
	"repository": {
		"type": "git",
		"url": "https://github.com/Fooxly/quick-terminal.git"
	},
	"bugs": {
		"url": "https://github.com/Fooxly/quick-terminal/issues",
		"email": "bugs@fooxly.com"
	},
	"license": "MIT",
	"main": "./out/extension.js",
	"engines": {
		"yarn": "^1",
		"vscode": "^1.39.0"
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"publisher": "Fooxly",
	"icon": "assets/icon.png",
	"galleryBanner": {
		"color": "#FAFAFA",
		"theme": "light"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Quick Terminal",
			"properties": {
				"quickterminal.priority": {
					"type": "integer",
					"title": "Status bar button priority",
					"description": "The priority given to the statusbar button",
					"default": 0
				}
			}
		}
	}
}
