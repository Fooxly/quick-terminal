{
	"name": "terminal",
	"displayName": "Terminal",
	"author": "Fooxly <hello@fooxly.com> (https://developers.fooxly.com/)",
	"publisher": "Fooxly",
	"icon": "assets/icon.png",
	"description": "Open an external terminal by the press of a button",
	"version": "0.0.2",
	"license": "MIT",
	"keywords": [
		"fooxly",
		"vscode",
		"code",
		"button",
		"statusbar",
		"terminal",
		"native"
	],
	"engines": {
		"yarn": "^1",
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
